{
    "private": true,
    "scripts": {
        "build": "npm run cleanup && npm run lint-fix && npx webpack --config ./webpack.prod.js --mode production",
        "start": "npm run cleanup && npm run lint-fix && TAILWIND_MODE=watch npx webpack --config ./webpack.dev.js",
        "cleanup:backend": "npx rimraf ../backend/public/{js,css,images,views} && npx rimraf ../backend/resources/views/pages",
        "cleanup:frontend": "npx rimraf ./public/{js,css,images,views}",
        "cleanup": "npm-run-all cleanup:{backend,frontend}",
        "lint:es": "npx eslint --fix './resources/**/*.{js,jsx,ts,tsx}'",
        "lint:style": "npx stylelint --fix './resources/**/*.{css,scss}'",
        "lint": "npm-run-all lint:{es,style}",
        "format": "npx prettier --write './resources/**/*.{js,jsx,ts,tsx,css,scss}'",
        "lint-fix": "npm run lint && npm rum format",
        "test": "jest"
    },
