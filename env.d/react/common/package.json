{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run lint-format && craco start",
    "build": "npm run lint-format && craco build",
    "test": "craco test",
    "eject": "craco eject",
    "lint:es": "npx eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "npx stylelint --fix './src/**/*.{css,scss}'",
    "lint": "npm-run-all lint:{es,style}",
    "format": "npx prettier --write './src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "lint-format": "npm run lint && npm rum format",
    "tsm": "npx typed-scss-modules src --implementation node-sass --nameFormat none --exportType default",
    "tsmw": "npx typed-scss-modules src --watch --implementation node-sass --nameFormat none --exportType default",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "@tanstack/react-query": "4.0.10",
    "@tanstack/react-query-devtools": "4.0.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^0.27.2",
    "connected-react-router": "^6.9.2",
    "history": "4.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.6.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.5",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.8",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^13.0.3",
    "msw": "^0.44.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "redux-logger": "^3.0.6",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^27.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "tailwindcss": "^3.1.8"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint-format"
  }
}
